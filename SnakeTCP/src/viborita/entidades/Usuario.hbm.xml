<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-mapping>
	<class name="viborita.entidades.Usuario" table="USUARIO">
		<id column="usuario" name="usuario" type="string" />
		<property column="contrasenia" name="contrasenia" type="string" />
	</class>
	
	<query name="NomUsuario"><![CDATA[SELECT u.usuario FROM Usuario u]]></query>
</hibernate-mapping>